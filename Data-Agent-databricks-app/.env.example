# Data-Agent local development
# Copy to .env and fill in values. Never commit .env to GitHub.

# OpenAI (required for agent)
OPENAI_API_KEY=

# Databricks SQL (required for query layer)
DATABRICKS_HOST=https://samsara-biztech-us-west-2.cloud.databricks.com
DATABRICKS_TOKEN=
DATABRICKS_WAREHOUSE_HTTP_PATH=/sql/1.0/warehouses/<warehouse-id>

# Server
PORT=3000

# Optional: admin protection for local dev
# ADMIN_SECRET=

# Optional: rate limit (requests per minute per session), 0 = disabled
# RATE_LIMIT_PER_MINUTE=0
